<section id='about-section'>
  <div class='about-section-box'>
    <div class='container'>
      <div class='row'>
        <div class='col-md-12'>
          <div class='about-section-textctr'>
            <div class='container'>
              <div class='row'>
                {{#each data}}
                  <div class='col-md-12'>
                    <p class='about-txt1'>{{this.title}}</p>
                    <p class='about-txt2'>{{this.description}}</p>
                  </div>
                {{/each}}
              </div>
            </div>

          </div>
        </div>

      </div>
    </div>
  </div>
</section>